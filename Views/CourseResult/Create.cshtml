@model MVC_Task.Services.ViewModels.CourseResultVM.CourseResultVMToAdd


@{
    ViewData["Title"] = "Create Course Result";
}

<h1>Create Course Result</h1>

<form asp-action="Create" method="post" asp-controller="CourseResult">
    <partial name="_CourseResultForm" />
    <br />
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
